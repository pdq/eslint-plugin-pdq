{
  "name": "@pdq/eslint-plugin-pdq",
  "description": "pdq's ESLint plugin and configurations",
  "version": "2.0.0",
  "author": "pdq",
  "license": "MIT",
  "main": "index.js",
  "files": [
    "index.js",
    "prettier-ts.js",
    "prettier.js",
    "rules"
  ],
  "scripts": {
    "eslint-check": "eslint --print-config . | eslint-config-prettier-check",
    "format": "prettier \"*.js\" \"rules/*.js\" --write && sortpack",
    "preversion": "npm t",
    "test": "run-s -s test:lint test:test",
    "test:lint": "eslint -c .eslintrc.json *.js rules",
    "test:test": "node test"
  },
  "homepage": "https://github.com/pdq/eslint-plugin-pdq#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pdq/eslint-plugin-pdq.git"
  },
  "bugs": {
    "url": "https://github.com/pdq/eslint-plugin-pdq/issues"
  },
  "keywords": [
    "config",
    "eslint",
    "eslint-config",
    "eslint-plugin",
    "eslintconfig",
    "eslintplugin",
    "lint",
    "pdq",
    "plugin"
  ],
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "3.5.0",
    "@typescript-eslint/parser": "3.5.0",
    "babel-eslint": "10.1.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-babel": "5.3.1",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jest": "23.17.1",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.20.2",
    "eslint-plugin-react-hooks": "4.0.5",
    "eslint-plugin-unicorn": "20.1.0"
  },
  "devDependencies": {
    "eslint": "5.16.0",
    "eslint-plugin-self": "1.2.1",
    "husky": "4.2.5",
    "lint-staged": "10.2.11",
    "npm-run-all": "4.1.5",
    "prettier": "2.0.5",
    "sortpack": "2.1.5"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "sortpack",
      "git add"
    ],
    "package.json": [
      "sortpack",
      "git add"
    ]
  },
  "peerDependencies": {
    "eslint": "^7.3.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
