{
  "name": "@pdq/eslint-plugin-pdq",
  "description": "pdq's ESLint plugin and configurations",
  "version": "2.0.9",
  "author": "pdq",
  "license": "MIT",
  "main": "index.js",
  "files": [
    "index.js",
    "prettier-ts.js",
    "prettier.js",
    "rules"
  ],
  "scripts": {
    "eslint-check": "eslint --print-config . | eslint-config-prettier-check",
    "format": "prettier \"*.js\" \"rules/*.js\" --write && sortpack",
    "prepare": "husky install",
    "preversion": "npm t",
    "test": "run-s test:lint test:test",
    "test:lint": "eslint -c .eslintrc.json *.js rules",
    "test:test": "node test"
  },
  "homepage": "https://github.com/pdq/eslint-plugin-pdq#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pdq/eslint-plugin-pdq.git"
  },
  "bugs": {
    "url": "https://github.com/pdq/eslint-plugin-pdq/issues"
  },
  "keywords": [
    "config",
    "eslint",
    "eslint-config",
    "eslint-plugin",
    "eslintconfig",
    "eslintplugin",
    "lint",
    "pdq",
    "plugin"
  ],
  "dependencies": {
    "@babel/eslint-plugin": "7.13.10",
    "@typescript-eslint/eslint-plugin": "4.18.0",
    "@typescript-eslint/parser": "4.18.0",
    "babel-eslint": "10.1.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.3.2",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-promise": "4.3.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-unicorn": "28.0.2"
  },
  "devDependencies": {
    "eslint": "7.22.0",
    "eslint-plugin-self": "1.2.1",
    "husky": "5.1.3",
    "lint-staged": "10.5.4",
    "npm-run-all": "4.1.5",
    "prettier": "2.2.1",
    "sortpack": "2.2.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write"
    ],
    "package.json": [
      "sortpack"
    ]
  },
  "peerDependencies": {
    "eslint": "^7.22.0"
  }
}
